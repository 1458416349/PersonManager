<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.pj.dao.ApplicantDao">
	<!--添加应聘人信息 -->
	<insert id="insertApplicantMes">
		insert into tb_invitejob
		(id,name,sex,age,born,job,specialty,experience,teachSchool,afterSchool,tel,address,createtime,content,isstock)
		values
		(#{id},#{name},#{sex},#{age},#{born},#{job},#{specialty},#{experience},#{teachSchool},#{afterSchool},
		#{tel},#{address},#{createtime},#{content},#{isstock})
	</insert>



</mapper>

